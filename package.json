{
  "name": "jate",
  "version": "1.0.0",
  "description": "text editor",
  "main": "server.js",
  "scripts": {
    "dev": "webpack-dev-server",
    "start:dev": "concurrently \"cd server && npm run server\" \"cd client && npm run dev\"",
    "start": "npm run build && cd server && node server.js",
    "server": "cd server nodemon server.js --ignore client",
    "build": "cd client && npm run build",
    "heroku-prebuild": "cd client && npm install --dev",
    "install": "cd client && npm install && cd ../server && npm install",
    "client": "cd client && npm start"
  },
  "keywords": [],
  "author": "Victoria",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^1.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stellalph/19-PWA-Text-Editor.git"
  },
  "bugs": {
    "url": "https://github.com/stellalph/19-PWA-Text-Editor/issues"
  },
  "homepage": "https://github.com/stellalph/19-PWA-Text-Editor#readme"
}